head(itemscope itemtype="http://schema.org/WebSite")

  mixin metaTitle(seo_data.title)
  //meta(name="google-site-verification" content="#{result.licensee.google_site_verification}")

  - if(noIndex)
    meta(name="robots", content="noindex, nofollow, noarchive")
  - else
    meta(name="robots", content="index, follow, noarchive")
  meta(http-equiv="Content-Type", content="text/html; charset=UTF-8")
  meta(charset="UTF-8")
  mixin metaDescription(seo_data.description)
  mixin metaKeywords(seo_data.keywords)

  meta(http-equiv="x-dns-prefetch-control" content="on")
  link(rel="dns-prefetch" href="http://www.res.cloudinary.com")
  link(rel="dns-prefetch" href="#{static}")
  link(rel="canonical" href="#{cannonical}" itemprop="url")
  meta(itemprop="url" content="#{cannonical}")

  if result.licensee
    link(rel="shortcut icon"  href="https://graph.facebook.com/#{result.licensee.facebook}/picture?width=80&height=80")

  mixin metaOG(seo_data, cannonical, author)

  link( href='#{static}/resources/themes/#{result.licensee.theme}/style.css' rel="stylesheet" type="text/css")
  link( href='#{static}/resources/css/TDG.css' rel="stylesheet" type="text/css")
  meta(name="viewport" content="width=device-width, height=device-height, initial-scale=1.0, maximum-scale=1.0, user-scalable=0")
  if ENV == 'prod'
  block css