-licensee = result.licensee
- pages = result.pages


.traveltour-mobile-header-wrap.row
  #traveltour-mobile-header.traveltour-mobile-header.traveltour-header-background.traveltour-style-slide
    .traveltour-mobile-header-container.traveltour-container
      .traveltour-logo.traveltour-item-pdlr
        .traveltour-logo-inner
          a(href='/')
            img(style="width:100%;max-width:65px;border-radius:100%" src="#{licensee.image_url}")
      .traveltour-mobile-menu-right
        #traveltour-mobile-top-search.traveltour-main-menu-search
          i.fa.fa-search
        .traveltour-top-search-wrap
          .traveltour-top-search-close
          .traveltour-top-search-row
            .traveltour-top-search-cell
              form.search-form(role='search', method='get', action='/')
                input.search-field.traveltour-title-font(type='text', placeholder='Search...', value='', name='s')
                .traveltour-top-search-submit
                  i.fa.fa-search
                input.search-submit(type='submit', value='Search')
                .traveltour-top-search-close
                  i.icon_close
        .traveltour-mobile-menu
          a.traveltour-mm-menu-button.traveltour-mobile-menu-button.traveltour-mobile-button-hamburger-with-border(href='#traveltour-mobile-menu')
            i.fa.fa-bars
          #traveltour-mobile-menu.traveltour-mm-menu-wrap.traveltour-navigation-font(data-slide='right')
            ul#menu-main-navigation.m-menu
              li.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-home.current-menu-ancestor.current-menu-parent.current_page_parent.current_page_ancestor.menu-item-has-children.menu-item-4373
                a(href='/') Home
              li.menu-item.menu-item-type-custom.menu-item-object-custom.menu-item-4341.traveltour-normal-menu
                a(href='/plan') Tours
              li.menu-item.menu-item-type-custom.menu-item-object-custom.menu-item-4341.traveltour-normal-menu
                a(href='/blog') Blog
              li.menu-item.menu-item-type-custom.menu-item-object-custom.menu-item-4341.traveltour-normal-menu
                i.fa.fa-phone(style='font-size: 16px;color: #ffffff;margin-right: 10px;')
              |  +91-#{licensee.mobile}
              li.menu-item.menu-item-type-custom.menu-item-object-custom.menu-item-4341.traveltour-normal-menu
                i.fa.fa-envelope-o(style='font-size: 16px;color: #ffffff;margin-left: 30px;margin-right: 10px;')
              |  #{licensee.email}
