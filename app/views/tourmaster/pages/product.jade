              .tourmaster-tour-booking-bar-inner.card.content
                .tourmaster-booking-tab-content.tourmaster-active
                      label.date-container
                        .small Pick your Start Date
                        input.form-control.js-startDate(type="text" placeholder="dd-mm-yyyy" data-date-format="dd-mm-yyyy" name="start_date" value="#{checkin}")
                    section
                      .small Package Cost Calculator/ Group offer
                      .row.text-center
                        .col-xs-3
                          div
                            small Price
                          .js-offer ₹#{parseInt(product.offer_price)}
                        .col-xs-5
                          div
                            small Travellers
                          div
                            input.js-priceFactor.js-count(type="hidden" name="travellers" placeholder="Your Group size" value="#{product.travellers}")
                            .qty-icon.js-qIcon(data-type="minus" style="display:inline ; margin-right:5px") -
                            .qty.js-quantity(style="display:inline;margin-right:5px") #{product.travellers}
                            .qty-icon.js-qIcon(data-type="plus" style="display:inline;") +
                        .col-xs-4
                          div
                            small Total
                          div ₹
                            span.js-total #{product.travellers * parseInt(product.offer_price)}
                    .js-book.tr-bookNow(data-slug="#{product.slug}")
                      .booking-btn
                        .err-txt.js-bookStatus
                        .btn.lg.btn-primary Proceed Booking
                  .tourmaster-lightbox-content-wrap(data-tmlb-id='proceed-without-login')
