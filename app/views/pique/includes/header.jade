-licensee = result.licensee
- pages = result.pages
- tags = result.tags
header.site-header.row(role='banner')
  .content-wrapper
    include ../../includes/mobileNav

    .traveltour-top-bar
    .traveltour-top-bar-background
    .traveltour-top-bar-container
      .traveltour-top-bar-left.traveltour-item-pdlr.hidden-xs
        i.fa.fa-phone(style='font-size: 16px;color: #ffffff;margin-right: 5px;')
          span.span(style="margin-left:5px")  +91-#{licensee.mobile}
        i.fas.fa-envelope(style='font-size: 16px;color: #ffffff;margin-left: 30px;margin-right: 10px;')
          span.span(style="margin-left:5px") #{licensee.email}
      .traveltour-top-bar-right.traveltour-item-pdlr
        .traveltour-top-bar-right-social.hidden-xs
          a.traveltour-top-bar-social-icon(href="https://www.facebook.com/#{licensee.facebook}", target='_blank', title='facebook')
            i.fab.fa-facebook
          a.traveltour-top-bar-social-icon(href="https://www.instagram.com/#{licensee.instagram}", target='_blank', title='instagram')
            i.fab.fa-instagram
          a.traveltour-top-bar-social-icon(href="https://www.pinterest.com/#{licensee.pinterest}", target='_blank', title='pin-interest')
            i.fab.fa-pinterest
          a.traveltour-top-bar-social-icon(href='https://www.twitter.com/#{licensee.twitter}', target='_blank', title='twitter')
            i.fab.fa-twitter
    .col-sm-2.col-xs-3.logo-wrap
      a(href="/"  title="Home")
        if licensee.image_url
          img(style="width:100%;max-width:85px;border-radius:100%" src="#{licensee.image_url}" alt="#{licensee.name}")
        else
          img(style="width:100%;max-width:85px;border-radius:100%" src="https://graph.facebook.com/#{licensee.facebook}/picture?type=large")
    #site-navigation-wrapper
          nav#site-navigation.main-navigation.hidden-xs(role='navigation')
            .menu-primary-container
              ul#primary-menu.menu
                li.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-home.current-menu-item.page_item.page-item-2.current_page_item.menu-item-12
                  a(href='/') Home
                li.menu-item.menu-item-type-post_type.menu-item-object-page
                  a(href='/blog') Blog
                //li.menu-item.menu-item-type-post_type.menu-item-object-page
                //  a(href='/plan') Tours
                li
                  if tags
                    a(href='/plan') Tours â–¾
                    ul.dropdown
                      each tours, tag in tags
                        li
                          a( href='#') &#9666; #{tag}
                          ul.dropdown-submenu
                            each tour in tours
                              li
                                a(href='/plan/#{tour.slug}') #{tour.name}
                  else
                    a(href='/plan') Tours
                li.menu-item.menu-item-type-post_type.menu-item-object-page
                  a(href='/event') Upcoming Events
                for page, index in pages
                   if index < 5
                       li.menu-item.menu-item-type-post_type.menu-item-object-page
                          a(href="/page/#{page.slug}") #{page.title}


    //.col-sm-10.text-right.hidden-xs
      div
        a.btn.btn-ghost(style="margin:10px;" href="/plan" title="Best tour deals") Tours
        a.btn.btn-ghost(style="margin:10px;" href="/#contact" title="Contact") Contact
        a.btn.btn-ghost(style="margin:10px;" href="/blog" title="Blog") Blog
        a.btn.btn-ghost(style="margin:10px;" href="/#reviews" title="Testimonial") Testimonial
        a.btn.btn-ghost(style="margin:10px;" href="/#choose" title="Why choose us") Why Choose Us
        a.btn.btn-ghost(style="margin:10px;" href="/#contact" title="Enquire") Enquire

      div Contact Us at&nbsp;
        a(href="tel:+91#{licensee.mobile}") +91-#{licensee.mobile}
    //.col-xs-9.text-right.visible-xs
      div
        a.btn.btn-ghost(href="#contact" title="Contact") Contact
        a.btn.btn-ghost(href="/plan" title="Blog") Tours
      div Contact Us at&nbsp;
        a(href="tel:+91#{licensee.mobile}") +91-#{licensee.mobile}
  
    .col-xs-12(style="min-height: 160px;")
        .site-branding
          h1.site-title #{licensee.name}
          p.site-description #{licensee.tagline}
